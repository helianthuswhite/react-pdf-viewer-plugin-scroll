{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "isolatedModules": true,
        "jsx": "react",
        "module": "es6",
        "moduleResolution": "node",
        "removeComments": true,
        "resolveJsonModule": true,
        "target": "es5",
        "baseUrl": ".",
    },
    "exclude": ["node_modules"]
}
